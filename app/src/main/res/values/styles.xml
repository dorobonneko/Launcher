<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme" parent="@style/BaseWallpaperTheme">
	</style>
	<style name="BaseWallpaperTheme" parent="@android:style/Theme.DeviceDefault.Light.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
	<style name="Preference" parent="@android:style/Theme.DeviceDefault.Light">
	</style>
	<style name="Settings" parent="Preference"/>
	<style name="Dialog" parent="@android:style/Theme.DeviceDefault.Light.Dialog.Alert"/>
</resources>
